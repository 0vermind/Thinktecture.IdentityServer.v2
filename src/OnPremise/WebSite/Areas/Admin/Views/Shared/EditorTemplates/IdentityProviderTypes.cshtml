@model Thinktecture.IdentityServer.Models.IdentityProviderTypes
@{
    var enumType = typeof(Thinktecture.IdentityServer.Models.IdentityProviderTypes);
    var list = new List<SelectListItem>();
    foreach(var name in Enum.GetNames(enumType))
    {
        var val = ((int)Enum.Parse(enumType, name));
        list.Add(new SelectListItem { Text = name, Value = val.ToString(), Selected=(val==(int)Model) });
    }
    
}
@Html.DropDownList("", list)
